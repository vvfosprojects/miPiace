{
  "ConnectionString": "$mongodbConString",
  "DatabaseName": "$mongodbName",
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "$logPath",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "$ServerKestrel"
      }
    }
  },
  "AllowedHosts": "*"
}